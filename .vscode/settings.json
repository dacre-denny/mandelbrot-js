{
  "eslint.options": {
    "rules": {
      "semi": [2, "always"],
      "quotes": [2, "double"],
      "padding-line-between-statements": [
        2,
        { "blankLine": "always", "prev": ["const", "let", "var"], "next": "*" },
        {
          "blankLine": "any",
          "prev": ["const", "let", "var"],
          "next": ["const", "let", "var"]
        }
      ],
      "padding-blocks": [2, "always"]
    }
  }
}
